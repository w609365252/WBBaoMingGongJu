.dialog {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 2;
}

.dialog-bg {
    width: 100%;
    height: 100%;
    background-color: #000;
    opacity: 0.6;
    position: absolute;
    z-index: 2;
}

.dialog-content {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: absolute;
    z-index: 3;
}

.dialog-close {
    width: 48rpx;
    height: 75rpx;
}

.dialog-view {
    position: relative;
    width: 590rpx;
    height: 840rpx;
}

.dialog-image {
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: 3;
}

.dialog-info {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: absolute;
    z-index: 4;
}

.dialog-title {
    margin: 62rpx 0 34rpx 0;
    color: #12b7f5;
    font-size: 40rpx;
}

.dialog-scroll-view {
    width: 460rpx;
    height: 610rpx;
    line-height: 1.6;
    color: #333;
    font-size: 28rpx;
}

.enroll-list {
    position: relative;
    padding: 0 30rpx;
    transition: all 0.4s;
}

.enroll-label {
    font-size: 30rpx;
    padding: 40rpx 0 30rpx;
    color: #333;
}

.enroll-delete-icon {
    position: absolute;
    top: 40rpx;
    right: 30rpx;
    width: 32rpx;
    height: 32rpx;
}

.enroll-panel {
    overflow: hidden;
}

.enroll-item {
    position: relative;
    background: #fff;
    padding: 20rpx;
    margin-bottom: 30rpx;
}

.border-top {
    border-top: 1rpx solid #eee;
    margin-bottom: 0;
}

.enroll-img {
    float: left;
    display: block;
    width: 150rpx;
    height: 150rpx;
    border-radius: 10rpx;
    background: #eee;
}

.enroll-body {
    position: relative;
    margin-left: 170rpx;
    height: 150rpx;
}

.enroll-title {
    display: box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-size: 32rpx;
    color: #333;
    line-height: 1.5;
}

.enroll-count {
    overflow: hidden;
    position: absolute;
    left: 0;
    bottom: 0;
}

.enroll-person {
    float: left;
    width: 24rpx;
    height: 24rpx;
    margin-top: 6rpx;
    margin-right: 12rpx;
}

.enroll-count view {
    float: left;
    white-space: nowrap;
    font-size: 26rpx;
    color: #999;
    line-height: 1.7;
}

.enroll-count view label {
    padding-left: 60rpx;
}

.enroll-status {
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    font-size: 26rpx;
    border: 1rpx solid #aaaaaa;
    color: #aaaaaa;
    line-height: 1.5;
    padding: 2rpx 10rpx;
}

.blur {
    border: 1rpx solid #00a8f3;
    color: #00a8f3;
}

.red {
    border: 1rpx solid #fa766f;
    color: #fa766f;
}

.btn-delete {
    position: absolute;
    display: flex;
    width: 180rpx;
    height: 190rpx;
    color: #fff;
    align-items: center;
    top: 0;
    justify-content: center;
    background-color: red;
    right: -180rpx;
    font-size: 30rpx;
    transition: all 0.4s;
}

.btn-more {
    position: absolute;
    top: 0;
    right: -16rpx;
    width: 50rpx;
    height: 50rpx;
}

.btn-more-dev {
    position: absolute;
    top: -6rpx;
    right: 0;
    width: 48rpx;
    height: 48rpx;
}

.icon-label {
    position: absolute;
    left: 0;
    bottom: 0;
    font-size: 26rpx;
    padding: 6rpx 8rpx;
    color: #fff;
    border-radius: 4rpx;
}

.icon-ju {
    background: #35bd3b;
}

.icon-ke {
    background: #FFC107;
}

.icon-xin {
    background: #8b34f6;
}

.icon-tuan {
    background: #ff6600;
}

.censor-tags {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 10;
    display: block;
    padding: 12rpx;
    text-align: center;
    background: #f10000;
    color: #fff;
    font-size: 28rpx;
}

.censor-tags.right-bottom {
    left: 62rpx;
    top: auto;
    bottom: 0;
}

.censor-tags-warning {
    background: #ffbe00;
}

.censor-tags-success {
    background: #35bd3b;
}

.censor-tags-right {
    left: auto;
    right: 54rpx;
}

.dialog-huoma,.dialog-huoma .mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 100;
}

.dialog-huoma .mask {
    background: rgba(0,0,0,.4);
}

.dialog-huoma .dialog-box {
    position: absolute;
    left: 50%;
    top: 50%;
    z-index: 101;
    transform: translate(-50%,-50%);
    width: 600rpx;
    padding: 40rpx;
    background: #fff;
    border-radius: 10rpx;
}

.dialog-huoma .dialog-title {
    text-align: center;
    font-size: 36rpx;
    color: #333;
    margin: 20rpx 0 30rpx;
}

.dialog-huoma .dialog-desc {
    color: #333;
    font-size: 28rpx;
    line-height: 1.7;
    padding-bottom: 30rpx;
}

.dialog-huoma .button {
    max-width: 100%;
    margin: 20rpx 0 0;
    line-height: 2.3;
}

.dialog-huoma .btn-white {
    background: #fff;
    border: 1rpx solid #ddd;
    color: #666;
}

.dialog-confirm,.dialog-confirm .mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 100;
}

.mask {
    background: rgba(0,0,0,.4);
}

.dialog-box {
    position: absolute;
    left: 50%;
    top: 50%;
    z-index: 101;
    transform: translate(-50%,-50%);
    width: 630rpx;
    background: #fff;
    border-radius: 10rpx;
}

.dialog-box .dialog-title {
    text-align: center;
    color: #333;
    font-size: 34rpx;
    margin-top: 50rpx;
}

.dialog-box .dialog-desc {
    color: #333;
    font-size: 30rpx;
    line-height: 1.7;
    margin: 0 30rpx;
}

.dialog-box .dialog-footer {
    display: flex;
    align-items: center;
    margin-top: 30rpx;
    padding-top: 0;
    border-top: 1rpx solid #eee;
}

.dialog-confirm .button {
    margin-bottom: 0;
    background: none;
    color: #333;
    font-size: 34rpx;
    height: 100rpx;
    line-height: 100rpx;
}

.dialog-box .btn-ok {
    color: #12b7f5;
}

.login {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    padding: 80rpx 0 0;
    background: #fff;
    text-align: center;
}

.login-logo {
    display: block;
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto 40rpx;
}

.login-title {
    font-size: 40rpx;
    color: #333;
    line-height: 1.5;
}

.login-desc {
    display: block;
    font-size: 28rpx;
    color: #999;
    line-height: 1.5;
    padding: 60rpx 0;
}

.btn-auth {
    display: flex!important;
    align-items: center;
    justify-content: center;
    width: 500rpx!important;
}

.btn-auth image {
    width: 42rpx;
    height: 42rpx;
    margin-top: 0!important;
    margin-right: 15rpx!important;
}

.login-contact {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 30rpx;
    text-align: center;
    overflow: hidden;
}

.login-contact view {
    font-size: 26rpx;
    color: #999;
    padding-bottom: 20rpx;
}

.login-contact label {
    display: block;
    color: #1677d2;
    font-size: 24rpx;
    margin-top: 10rpx;
}

.login-contact button {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 60rpx;
    opacity: 0;
    background: #ccc;
    z-index: 10;
}

.recommend-item image {
    display: block;
    margin-bottom: 20rpx;
}

.ui-field-type-box {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 1000;
}

.ui-field-type-box cover-view {
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: -1;
    background: rgba(0,0,0,.4);
    display: block!important;
}

.ui-field-content {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10000;
    background: #fff;
}

.ui-field-content cover-view {
    position: absolute;
    z-index: 0;
    width: 100%;
    height: 100%;
}

.ui-field-header {
    padding: 20rpx 0;
}

.ui-field-header image {
    display: block;
    margin: 0 auto;
    width: 44rpx;
    height: 44rpx;
}

.ui-field-body {
    padding: 0 30rpx 30rpx;
}

.ui-field-body .ui-field-title {
    font-size: 26rpx;
    color: #666;
    padding-bottom: 20rpx;
}

.ui-field-list {
    overflow: hidden;
    border: 1rpx solid #ccc;
}

.ui-field-type-item {
    float: left;
    width: 33.333333%;
    display: flex;
    align-items: center;
    padding: 25rpx 0 25rpx 20rpx;
}

.ui-field-type-item image {
    display: block;
    width: 36rpx;
    height: 36rpx;
}

.ui-field-type-item label {
    font-size: 32rpx;
    color: #333;
    padding-left: 10rpx;
}

.ui-field-type-item .last {
    color: #999;
}

.enroll-demo {
    margin-top: 20rpx;
    background: #fff;
    padding: 20rpx 30rpx 30rpx;
}

.enroll-demo-title {
    font-size: 28rpx;
    color: #333;
    padding-bottom: 10rpx;
}

.enroll-demo-bd {
    position: relative;
    display: flex;
    align-items: center;
    background: #fff;
}

.enroll-demo-bd image {
    width: 76rpx;
    height: 76rpx;
}

.enroll-demo-right {
    width: 460rpx;
    margin-right: 100rpx;
    padding-left: 20rpx;
    overflow: hidden;
    text-align: left;
}

.enroll-demo-right view {
    font-size: 30rpx;
    color: #333;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.enroll-demo-right label {
    display: block;
    font-size: 24rpx;
    color: #999;
    line-height: 1.5;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.btn-demo {
    position: absolute;
    right: 0;
    top: 50%;
    transform: translate(0,-50%);
    font-size: 28rpx;
    color: #fff;
    background: #12b7f5;
    border-radius: 10rpx;
    padding: 12rpx 25rpx;
}

.follow-tips {
    font-size: 28rpx;
    color: #333;
}

.btn-follow {
    margin-top: 30rpx;
    margin-bottom: 0!important;
    border-radius: 50rpx!important;
    font-size: 30rpx!important;
    line-height: 86rpx!important;
}

.follow-dialog .d-close {
    width: 30rpx;
    height: 30rpx;
    opacity: .6;
    padding: 30rpx;
}

@media screen and (min-width:769px) {
    .login {
        padding-top: 20rpx;
    }

    .login-logo {
        width: 100rpx;
        height: 100rpx;
        margin-bottom: 20rpx;
    }

    .login-title {
        font-size: 20rpx;
    }

    .login-desc {
        padding: 20rpx 0;
        font-size: 16rpx;
    }

    .login .button {
        height: 66rpx;
        font-size: 30rpx;
    }

    .login-contact {
        bottom: 20rpx;
    }

    .login-contact view {
        font-size: 16rpx;
        padding-bottom: 8rpx;
    }

    .login-contact label {
        font-size: 16rpx;
    }

    .login-contact button {
        height: 40rpx;
        font-size: 16rpx;
    }
}