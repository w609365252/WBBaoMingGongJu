.wrapper {
    text-align: center;
    padding: 80rpx 0 30rpx;
}

icon {
    margin: 0 auto 60rpx;
}

.rs-title {
    font-size: 40rpx;
    color: #333;
    line-height: 1.5;
    padding-bottom: 20rpx;
}

.rs-subtitle {
    display: block;
    font-size: 30rpx;
    color: #999;
    padding: 0 80rpx 60rpx;
    line-height: 1.6;
    white-space: normal;
    word-break: break-all;
}

.pb0 {
    padding-bottom: 0;
}

.rs-subtitle label {
    color: #fc776d;
    padding: 0 10rpx;
}

.btn-white {
    background: #fff;
    color: #666;
    border: 1rpx solid #ccc;
}

.ads-item {
    position: fixed;
    left: 30rpx;
    right: 30rpx;
    bottom: 50rpx;
}

.ads-item image {
    width: 100%;
    height: 100rpx;
}