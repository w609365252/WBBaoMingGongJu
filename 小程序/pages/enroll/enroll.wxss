input {
    box-sizing: border-box;
}

.top-tips {
    padding-top: 0!important;
    background: #faf3c7;
}

.top-tips .enroll-info-flex {
    background: none;
    font-size: 26rpx;
    color: #b88d3a;
}

.enroll-info {
    padding-top: 20rpx;
}

.enroll-info .label {
    font-size: 30rpx;
    color: #333;
    padding: 0 30rpx 12rpx;
}

.desc {
    font-size: 26rpx;
    color: #999;
    padding: 0 30rpx 12rpx;
    margin-top: -10rpx;
}

.label label {
    display: inline-block;
    color: #ff6d6d;
    padding-left: 10rpx;
    font-size: 40rpx;
    height: 20rpx;
    line-height: 30rpx;
    overflow: hidden;
}

.form-control {
    display: block;
    background: #fff;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 30rpx;
    padding: 0 30rpx;
}

.form-control label {
    color: #ff6d6d;
}

.radio-group {
    background: #fff;
}

.radio {
    display: block;
    border-bottom: 1rpx solid #f5f5f5;
    padding: 15rpx 30rpx;
    font-size: 30rpx;
    line-height: 2;
}

.enroll-tips {
    font-size: 24rpx;
    color: #999;
    padding: 30rpx;
    white-space: nowrap;
}

.enroll-tips image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
    vertical-align: top;
}

.btn-exit {
    background: #fff;
    color: #000;
    border: 1rpx solid #ddd;
}

.enroll-info-item {
    background: #fff;
    min-height: 88rpx;
    line-height: 88rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    overflow: hidden;
    border-bottom: 1rpx solid #e5e5e5;
    color: black;
    font-size: 30rpx;
}

.enroll-info-item .text {
    float: left;
    color: black;
    font-size: 30rpx;
}

.enroll-info-item picker {
    position: relative;
}

.enroll-info-item picker view {
    width: 100%;
    height: 88rpx;
}

.enroll-info-item .arrow {
    position: absolute;
    right: 0;
    top: 50%;
    transform: translate(0,-50%);
    width: 16rpx;
    height: 25rpx;
}

.enroll-info-item textarea {
    margin: 10rpx 0;
    padding: 0;
    line-height: 1.5;
    font-size: 30rpx;
    width: 690rpx;
    height: 180rpx;
}

.address {
    position: relative;
    line-height: 40rpx;
}

.address .text {
    float: none;
    padding-top: 20rpx;
    line-height: 1.5;
}

.address .clear {
    position: absolute;
    right: 0;
    top: 0;
    padding: 20rpx 30rpx;
}

.btn-address {
    text-align: right;
}

.btn-address button,.btn-phone {
    display: inline-block;
    background: #00a8f3;
    color: #fff;
    font-size: 28rpx;
    padding: 10rpx;
    border-radius: 10rpx;
    margin: 20rpx 0;
}

.enroll-info-flex {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 90rpx;
    background: #fff;
    padding-left: 30rpx;
    padding-right: 30rpx;
    overflow: hidden;
    border-bottom: 1rpx solid #e5e5e5;
    color: black;
    font-size: 30rpx;
}

.enroll-info-flex label {
    color: #666;
    font-size: 26rpx;
}

.enroll-info-flex .red {
    color: #b88d3a;
    border: 0;
    font-weight: 900;
}

.right-arrow {
    width: 16rpx;
    height: 25rpx;
    margin-left: 16rpx;
    margin-top: 4rpx;
}

.vote-info-item .right {
    float: right;
    color: black;
    font-size: 30rpx;
}

.enroll-info-img {
    background: #fff;
    overflow: hidden;
    padding-bottom: 20rpx;
    padding-left: 30rpx;
}

.img-item {
    position: relative;
    float: left;
    margin: 20rpx 30rpx 0 0;
}

.img-item .minus {
    position: absolute;
    left: -12rpx;
    top: -12rpx;
    width: 30rpx;
    height: 30rpx;
    z-index: 1;
    padding-bottom: 18rpx;
    padding-right: 18rpx;
}

.big-img {
    border: 1rpx solid #dbdcdd;
    width: 150rpx;
    height: 150rpx;
}

.footer {
    padding: 10rpx 0;
}

.button label {
    padding-left: 16rpx;
}

.btn-pay {
    display: flex;
    align-items: center;
    justify-content: center;
}

.weixin-pay {
    margin-right: 10rpx;
    width: 48rpx;
    height: 48rpx;
}

.show {
    display: block;
}

.hide {
    display: none;
}

.text-red {
    color: #ff6d6d;
}

.ads-item {
    width: 100%;
}

.ads-item image {
    width: 100%;
    height: 100rpx;
}

.dialog-enroll-body {
    width: 550rpx;
    text-align: center;
    overflow: hidden;
    padding: 75rpx 40rpx 40rpx;
}

.enroll-success-close {
    position: absolute;
    top: 30rpx;
    right: 20rpx;
    width: 36rpx;
    height: 36rpx;
}

.dialog-enroll-body .icon {
    margin-bottom: 30rpx;
}

.enroll-success-title {
    font-size: 40rpx;
    color: #333;
    padding-bottom: 20rpx;
}

.enroll-success-desc {
    font-size: 30rpx;
    color: #999;
    padding-bottom: 40rpx;
}

.dialog-enroll-body .button {
    max-width: 100%;
}

.goods-label {
    font-size: 30rpx;
    color: #333;
    padding: 20rpx 30rpx 12rpx;
}

.goods-box {
    background: #fff;
    padding: 20rpx 30rpx;
}

.good-item {
    position: relative;
    border: 1rpx solid #ccc;
    height: 200rpx;
    overflow: hidden;
    display: flex;
    margin-bottom: 20rpx;
}

.good-pic {
    display: block;
    width: 200rpx;
    height: 200rpx;
}

.good-body {
    flex: 1;
    padding: 20rpx 20rpx;
}

.good-title {
    font-size: 30rpx;
    line-height: 1.3;
    color: #333;
    font-weight: 900;
    width: 430rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.good-desc {
    font-size: 24rpx;
    color: #999;
    line-height: 1.4;
    padding-top: 10rpx;
    max-width: 440rpx;
    word-break: break-all;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.good-bottom {
    position: absolute;
    right: 20rpx;
    bottom: 16rpx;
    width: 450rpx;
    display: flex;
    align-items: center;
}

.good-price {
    font-size: 30rpx;
    color: #f00;
    margin-right: 10rpx;
}

.good-remain {
    font-size: 24rpx;
    color: #999;
}

.count-box {
    position: absolute;
    right: 0;
    bottom: -10rpx;
    display: flex;
    align-items: center;
}

.count-box image {
    width: 30rpx;
    height: 30rpx;
    padding: 20rpx 15rpx;
}

.count-box view {
    font-size: 30rpx;
    color: #333;
    width: 60rpx;
    text-align: center;
}

.count-buy {
    position: absolute;
    right: 20rpx;
    bottom: 0;
    color: #666;
    font-size: 24rpx;
}

.total {
    text-align: right;
    font-size: 30rpx;
    color: 999;
}

.total label {
    color: #f00;
}

.pic-count {
    position: absolute;
    left: 0;
    top: 0;
    z-index: 8;
    background: rgba(0,0,0,0.4);
    color: #fff;
    font-size: 24rpx;
    padding: 0 10rpx;
    height: 36rpx;
    line-height: 36rpx;
}

.enroll-tab-box {
    padding: 12rpx 0 0 10rpx;
}

.enroll-tab {
    position: relative;
    padding: 0 20rpx;
    height: 64rpx;
    line-height: 64rpx;
    margin-left: 20rpx;
    margin-bottom: 10rpx;
    border: 1rpx solid #999;
    color: #999;
    font-size: 26rpx;
    float: left;
    white-space: nowrap;
}

.enroll-limit {
    height: 62rpx;
    line-height: 62rpx;
}

.enroll-tab.selected {
    border: 1rpx solid #12b7f5;
    color: #12b7f5;
}

.enroll-tab image {
    width: 24rpx;
    height: 22rpx;
    position: absolute;
    right: 0;
    bottom: 0;
}

.bottom-tips {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 10rpx 0 40rpx;
    font-size: 24rpx;
    color: #999;
}

.bottom-tips navigator {
    color: #576b95;
    font-style: italic;
    text-decoration: underline;
    margin-left: 10rpx;
}

.btn-phone {
    position: absolute;
    right: 30rpx;
    bottom: 0rpx;
    z-index: 10;
}