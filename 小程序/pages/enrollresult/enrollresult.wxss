.wrapper {
    text-align: center;
    padding: 50rpx 0 20rpx;
    background: #fff;
    margin-bottom: 20rpx;
}

icon {
    display: block;
    margin: 0 auto 30rpx;
}

.rs-icon {
    display: block;
    margin: 0 auto 30rpx;
    width: 120rpx;
    height: 120rpx;
}

.rs-title {
    font-size: 40rpx;
    color: #333;
    line-height: 1.5;
    padding-bottom: 10rpx;
}

.rs-subtitle {
    display: block;
    font-size: 30rpx;
    color: #999;
    padding: 0 70rpx 30rpx;
    line-height: 1.6;
}

.pb0 {
    padding-bottom: 0;
}

.rs-subtitle label {
    color: #fc776d;
    padding: 0 10rpx;
}

.result-wait {
    display: block;
    width: 120rpx;
    height: 120rpx;
    margin: 0 auto 30rpx;
}

.btn-white {
    background: #fff;
    color: #666;
    border: 1rpx solid #ccc;
    margin-bottom: 0;
}

.btn-hidden {
    visibility: hidden;
}

.ads-item {
    position: fixed;
    left: 30rpx;
    right: 30rpx;
    bottom: 50rpx;
}

.ads-item image {
    width: 100%;
    height: 100rpx;
}

.enroll-demo-bd {
    position: relative;
    border-top: 1rpx solid #ddd;
    padding: 20rpx 30rpx 0;
}

.btn-demo {
    right: 30rpx;
    top: 61%;
}

.enroll-demo-right {
    text-align: left;
}

.btn-more-box {
    margin-bottom: 20rpx;
}